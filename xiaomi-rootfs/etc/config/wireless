
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'pci0000:00/0000:00:00.0/0000:01:00.0'
	option txpower '20'
	option country '00'
	option hwmode '11nac'
	option channel '36'

config wifi-iface
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'OpenWrt_CHENG_5G'
	option wmm '0'
	option key '12345678'
	option encryption 'psk+ccmp'
	option disabled '1'

config wifi-device 'radio1'
	option type 'mac80211'
	option hwmode '11g'
	option path '10180000.wmac'
	option txpower '20'
	option country '00'
	option channel 'auto'

config wifi-iface
	option device 'radio1'
	option network 'lan'
	option mode 'ap'
	option ssid 'OpenWrt_CHENG'
	option key '12345678'
	option encryption 'none'

